@model IEnumerable<Lamarque_ciudad.map>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_map.cshtml";
}

<section>
    <div id="map"></div>
    <script>
        function initMap() {
            var uluru = { lat: -39.424004, lng: -65.701442 };
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 15,
                center: uluru
            });
            var marker = new google.maps.Marker({
                position: uluru,
                map: map
            });
        }
    </script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDXPWCsx8Us3pK_JQ0lcBSqugI_9Jf3NMQ&callback=initMap">
    </script>
</section>

